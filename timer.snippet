<?xml version="1.0" encoding="utf-8"?>
<CodeSnippets>
   <CodeSnippet Format="1.0.0">
      <Header>
         <Title>Timer-Snippet</Title>
         <Text>timer</Text>
         <Shortcut>timer</Shortcut>
         <Shortcut>Timer</Shortcut>
         <Shortcut>TIMER</Shortcut>
         <Extensions>.src .sub</Extensions>
         <Scope>InFunction InDeclarationList</Scope>
         <Description>Codeausschnitt f√ºr das starten eines Timers</Description>
         <Author>* WGS</Author>
         <SnippetTypes>
            <SnippetType>Expansion</SnippetType>
         </SnippetTypes>
      </Header>
      <Snippet>
         <Declarations>
            <Literal>
               <ID>timerIndex</ID>
               <ToolTip>Der Index des zu startendes Timers</ToolTip>
               <Default>Index</Default>
            </Literal>
         </Declarations>
         <Code Language="KRL">
            <![CDATA[$TIMER_STOP[$timerIndex$] = FALSE$end$]]>
         </Code>
      </Snippet>
   </CodeSnippet>
   
   <CodeSnippet Format="1.0.0">
      <Header>
         <Title>TimerOFF_ON-Snippet</Title>
         <Text>timerOFF_ON_on</Text>
         <Shortcut>timeroff_on</Shortcut>
         <Shortcut>Timeroff_on</Shortcut>
         <Shortcut>TIMEROFF_ON</Shortcut>
         <Extensions>.src .sub</Extensions>
         <Scope>InFunction InDeclarationList</Scope>
         <Description>Timer TOF ON</Description>
         <Author>* WGS</Author>
         <SnippetTypes>
            <SnippetType>Expansion</SnippetType>
         </SnippetTypes>
      </Header>
      <Snippet>
         <Declarations>
            <Literal>
               <ID>timerIndex</ID>
               <ToolTip>Der Index des zu startendes Timers</ToolTip>
               <Default>Index</Default>
            </Literal>
			<Literal>
               <ID>time</ID>
               <ToolTip>Time</ToolTip>
               <Default>1000</Default>
            </Literal>
         </Declarations>
         <Code Language="KRL">
            <![CDATA[	TimerOFF_ON($timerIndex$, $time$)$end$]]>
         </Code>
      </Snippet>
   </CodeSnippet>   

